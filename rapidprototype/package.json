{
  "name": "@rapidprototype-monorepo/rapidprototype",
  "version": "1.0.0",
  "description": "A rapid prototyping project for LLM workflow and agent development. A Typescript boilerplate with OpenRouter and Vercel AI SDK for quick LLM Prompt and Agent experimentation and development. Leverages scalable but light libraries and architectural patterns, with helpers. ",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "script": "yarn workspace @rapidprototype-monorepo/rapidprototype tsx src/run-script.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'"
  },
  "dependencies": {
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "ai": "^5.0.28",
    "axios": "^1.8.4",
    "bottleneck": "^2.19.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.18.3",
    "node-cron": "^3.0.3",
    "prompt-sync": "^4.2.0",
    "yargs": "^18.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.14",
    "@types/prompt-sync": "^4.2.3",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsx": "^4.19.1",
    "typescript": "^5.0.0"
  }
}
